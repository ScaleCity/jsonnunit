{
  "name": "jsonnunit",
  "version": "1.4.0",
  "description": "Simple and flexible jsonnet test framework in node.js",
  "author": "ScaleCity <info@scalecity.io>",
  "contributors": [
    "Levi Govaerts <levi@scalecity.io>"
  ],
  "bin": {
    "jsonnunit": "bin/run"
  },
  "bugs": "https://github.com/scalecity/jsonnunit/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "lodash": "^4.17.21",
    "lookpath": "^1.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4.3.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "lerna": "^4.0.0",
    "mocha": "^5",
    "nyc": "^14"
  },
  "engines": {
    "node": ">=8.5.0"
  },
  "files": [
    "/bin",
    "/src",
    "/jsonnet"
  ],
  "homepage": "https://github.com/scalecity/jsonnunit",
  "keywords": [
    "oclif",
    "jsonnet",
    "jsonnunit",
    "bdd",
    "tdd",
    "testing",
    "assertion"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "bin": "jsonnunit",
    "plugins": [
      "@oclif/plugin-update",
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "scalecity/jsonnunit",
  "scripts": {
    "posttest": "eslint src",
    "prepack": "oclif-dev readme",
    "preversion": "yarn test",
    "postversion": "git push --tags && yarn publish . --tag $npm_package_version && git push && echo \"Successfully released version $npm_package_version!\"",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
